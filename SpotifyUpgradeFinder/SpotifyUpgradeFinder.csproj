<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <OutputType>Exe</OutputType>
    <Authors>Nicholas Botticelli</Authors>
    <Company />
    <Description>Find download links for specific versions of Spotify (Windows)</Description>
    <PackageProjectUrl>https://github.com/nick-botticelli/SpotifyUpgradeFinder</PackageProjectUrl>
    <Copyright>Copyright © Nicholas Botticelli 2020</Copyright>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <PlatformTarget>AnyCPU</PlatformTarget>
  </PropertyGroup>
</Project>